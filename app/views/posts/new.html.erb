<%= form_with(model: @post, url: user_posts_path, method: :post) do |form| %>
    <h1>Create Post</h1>
    <%=form.label :title%>
    <%=form.text_field :title, class: 'post-text-field'%>
    <%=form.label "Post"%>
    <%=form.text_area :text, class: 'post-text-area'%>
    <%=form.submit "Post", class: 'common-btn'%>
    <%end%> 